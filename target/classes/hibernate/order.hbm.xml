<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping SYSTEM
        "classpath://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping xmlns = "http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="servletProject.entity.Order" table="orders">
        <id name="id">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator">
                <param name="sequence">order_id_seq</param>
            </generator>
        </id>

        <property name="person_id"></property>
        <property name="model_id"></property>
        <property name="car_id"></property>
        <property name="total_price"></property>
    </class>
</hibernate-mapping>
